<p id="notice"><%= notice %></p>

<div class="center">
  <p>
    <strong>Title:</strong>
    <%= @question_set.title %>
  </p>

  <ul>
    <% @question_set.questions.each do |question| %>
      <li><%= question.content %></li>
    <% end %>
  </ul>


  <%= button_to 'Add a Question', new_question_path, method: 'get', :form => { :id => 'add_a_question' } %>


  <%= link_to 'Edit', edit_question_set_path(@question_set) %> |
  <%= link_to 'Back', question_sets_path %>

</div>