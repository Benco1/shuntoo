<h1>Menu</h1>

<div class="center">
  <ul>
    <% @question_sets.each do |question_set| %>
      <li>
        <span><%= link_to "#{question_set.title}", question_set %></span>
        <span><%= link_to 'Show', question_set %></span>
        <span><%= link_to 'Edit', edit_question_set_path(question_set) %></span>
        <span><%= link_to 'X', question_set, method: :delete, data: { confirm: 'Are you sure?' } %></span>
      </li>
    <% end %>
  </ul>
  <br>
  <%= link_to 'New Project', new_question_set_path %>
</div>
